<?xml version="1.0" encoding="UTF-8"?>

<?import com.practice.lcn.jfx_mp3_player.MainApp?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:id="root" stylesheets="@/css/help/about.css">
    <children>
        <VBox spacing="10" AnchorPane.topAnchor="20.0" AnchorPane.leftAnchor="20.0">
            <children>
                <Label fx:id="labelTitle" text="" />
                <Label fx:id="labelVersion" text="" />
                <Label fx:id="labelAuthor" text="" />
            </children>
        </VBox>
        <HBox spacing="10" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0">
            <children>
                <Button fx:id="btnClose" text="Close" onAction="#handleBtnCloseClick" />
            </children>
        </HBox>
    </children>
</AnchorPane>
